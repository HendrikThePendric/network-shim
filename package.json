{
    "name": "network-shim",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@testing-library/jest-dom": "^5.11.4",
        "@testing-library/react": "^11.1.0",
        "@testing-library/user-event": "^12.1.10",
        "axios": "^0.21.0",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-scripts": "4.0.1",
        "web-vitals": "^0.2.4"
    },
    "devDependencies": {
        "concurrently": "^5.3.0",
        "cypress": "^6.0.0",
        "json-server": "^0.16.3",
        "json-server-reset": "^1.3.0",
        "xhook": "^1.4.9"
    },
    "peerDependencies": {
        "cypress": "^6.0.0"
    },
    "scripts": {
        "serve": "json-server src/json-server/db.js --routes ./src/json-server/routes.json --middlewares ./node_modules/json-server-reset --watch -p 1337",
        "cy:open": "cypress open",
        "cy:run": "cypress run",
        "start": "concurrently --kill-others \"yarn serve\" \"react-scripts start\" \"yarn cy:open\"",
        "capture": "concurrently --kill-others \"yarn serve\" \"react-scripts start\" \"yarn cy:run\"",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
